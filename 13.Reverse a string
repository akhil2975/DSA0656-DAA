#include<stdio.h>
#include<string.h>
int main()
{
	printf("Akhil (192211208)\n");
	char str[100];
	printf("Enter the string:");
	scanf("%s",&str);

	int len = strlen(str);
	for (int i = 0, j = len - 1; i <= j; i++, j--) {
		
		char c = str[i];
		str[i] = str[j];
		str[j] = c;
	}

	printf("Reversed String: %s", str);

	return 0;
}
